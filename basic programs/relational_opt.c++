// Relational Operator

#include <iostream>
using namespace std;

int main() {
    
    cout << (3 < 5) << endl;  //true -> 1
    cout << (3 > 5) << endl;  //false -> 0
    cout << (3 <= 5) << endl;
    cout << (3 >= 5) << endl;
    cout << (3 == 5) << endl;
    cout << (3 != 5) << endl;
    return 0;
}
